@using Oils.Models.ViewModels
@model EditReceiverEntityViewModel

<h1 class="text-center suls-text-color">Edit Receiver</h1>
<form asp-controller="Nomenclatures" asp-action="EditReceiver" method="post">

    <div class="container-fluid">
        <div class="row">
            <div class="col-4">

                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>

            </div>
            <div class="col-4">


            </div>
        </div>
    </div>

    <div class="top-border-line primary-separator"></div>
    <div class="button-holder d-flex justify-content-center">
        <button type="submit" class="btn btn-warning">Save changes</button>
    </div>
</form>



